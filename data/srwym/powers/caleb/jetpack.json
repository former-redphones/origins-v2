{
    "type": "origins:multiple",

    "jetpack": {
        "type": "origins:active_self",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "srwym:caleb/jetpack_fuel",
                    "change": -1.5
                },
                {
                    "type": "origins:add_velocity",
                    "y": 0.1,
                    "x": 0,
                    "z": 0
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:entity.warden.step",
                    "volume": 0.5,
                    "pitch": 0.1
                },
                {
                    "type": "origins:spawn_particles",
                    "particle": "minecraft:smoke",
                    "count": 5,
                    "speed": 0.1,
                    "spread": {
                        "x": 0.1,
                        "y": 0.0,
                        "z": 0.1
                    },
                    "offset_x": 0.0,
                    "offset_y": -0.5,
                    "offset_z": 0.0
                },
                {
                    "type": "origins:spawn_particles",
                    "particle": "minecraft:flame",
                    "count": 5,
                    "speed": 0.05,
                    "spread": {
                        "x": 0.0,
                        "y": 0.0,
                        "z": 0.0
                    },
                    "offset_x": 0.0,
                    "offset_y": 0.0,
                    "offset_z": 0.0
                }
            ]
        },
        "condition": {
            "type": "origins:resource",
            "resource": "srwym:caleb/jetpack_fuel",
            "comparison": ">",
            "compare_to": 0
        },
        "key": {
            "key": "key.origins.primary_active",
            "continuous": true
        }
    },

    "fuel": {
        "type": "origins:resource",
        "min": 0,
        "max": 40,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_03.png",
            "bar_index": 24
        }
    },

    "recharge": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "srwym:caleb/jetpack_fuel",
            "change": 1
        },
        "condition": {
            "type": "origins:on_block"
        }
    },

    "name": "Jetpack",
    "description": "Boost upwards at will!"
}